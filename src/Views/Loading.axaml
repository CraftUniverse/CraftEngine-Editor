<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" CanResize="False" SizeToContent="WidthAndHeight" Title="Loading..."
        x:Class="dev.craftengine.editor.Views.Loading" Closing="Window_OnClosing"
        WindowStartupLocation="CenterOwner" ShowInTaskbar="False">
    <StackPanel Margin="10">
        <ProgressBar IsIndeterminate="True" MinHeight="17"
                     MinWidth="350" ShowProgressText="False">
        </ProgressBar>
        <TextBlock Margin="0 5 0 0"
                   Name="Text" IsVisible="False">
        </TextBlock>
    </StackPanel>
</Window>
<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" SizeToContent="WidthAndHeight" CanResize="False"
        WindowStartupLocation="CenterOwner"
        x:Class="dev.craftengine.editor.Views.MessageBox">
    <StackPanel Margin="5">
        <TextBlock Margin="5" Name="Text" MaxWidth="550"></TextBlock>
        <StackPanel Orientation="Horizontal" Margin="0 5 0 0" HorizontalAlignment="Right">
            <Button Margin="5 0">Yes</Button>
            <Button Margin="5 0">No</Button>
        </StackPanel>
    </StackPanel>
</Window>
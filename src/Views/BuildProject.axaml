<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="400" Height="105"
        WindowStartupLocation="CenterOwner" CanResize="False"
        x:Class="dev.craftengine.editor.Views.BuildProject">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <StackPanel Margin="5">
                <TextBlock Margin="0 0 0 5">Export Path</TextBlock>
                <TextBox MinWidth="355"></TextBox>
            </StackPanel>
            <Button Margin="0 0 0 -21">...</Button>
        </StackPanel>
        <Button Margin="5">Build Project</Button>
    </StackPanel>
</Window>